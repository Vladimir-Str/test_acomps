@page
@model test_acomps.Pages.Test2.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>По-узловое сложение связанных списков</h1>

<p>
    <a asp-page="Create">Добавить числа для сложения</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Test2_model[0].Str1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Test2_model[0].Str2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Test2_model[0].Result)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Test2_model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Str1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Str2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Result)
            </td>
        </tr>
}
    </tbody>
</table>
